{
  "name": "schemas-lib",
  "version": "0.1.7",
  "author": "Simple Organization",
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "rollup -c -w",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "mocha **/*.spec.ts -r esbuild-register -extension ts --file tests/setup-tests.ts",
    "prepack": "pnpm build"
  },
  "main": "dist/index.js",
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "esbuild": "^0.18.13",
    "esbuild-register": "^3.4.2",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.vite.js"
  ],
  ".": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js"
  },
  "/vite": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.vite.js"
  },
  "types": "dist/index.d.ts",
  "type": "module"
}
